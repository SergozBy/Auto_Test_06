{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "first tests with Cypress",
  "main": "index.js",
  "scripts": {
    "test:default": "echo \"Error: no test specified\" && exit 1",
    "test": "cypress run",
    "open": "cypress open",
    "test:pc": "npx cypress run --config-file cypress.pc.config.js",
    "test:mobile": "npx cypress run --config-file cypress.mobile.config.js",
    "test:pc:chrome:headed": "npx cypress run --config-file cypress.pc.config.js --browser chrome --headed",
    "test:pc:chrome:headless": "npx cypress run --config-file cypress.pc.config.js --browser chrome",
    "test:pc:firefox:headed": "npx cypress run --config-file cypress.pc.config.js --browser firefox --headed",
    "test:pc:firefox:headless": "npx cypress run --config-file cypress.pc.config.js --browser firefox",
    "test:mobile:chrome:headed": "npx cypress run --config-file cypress.mobile.config.js --browser chrome --headed",
    "test:mobile:chrome:headless": "npx cypress run --config-file cypress.mobile.config.js --browser chrome",
    "test:mobile:firefox:headed": "npx cypress run --config-file cypress.mobile.config.js --browser firefox --headed",
    "test:mobile:firefox:headless": "npx cypress run --config-file cypress.mobile.config.js --browser firefox"
  },
  "author": "OM",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^14.3.2"
  }
}
